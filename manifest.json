{
    "manifest_version": 3,
    "id": "xyz.docsystem.betterdiscutaille",
    "name": "Better Discutaille",
    "version": "1.0",
    "description": "A better version of Discutaille",
    "content_scripts": [{
        "matches": ["https://discutaille.center/*"],
        "js": ["content.js"]
    }],
    "web_accessible_resources": [{
        "resources": ["betterdiscutaille.js", "betterdiscutaille.css", "librairies/*"],
        "matches": ["https://discutaille.center/*"]
    }],
    "icons": {
        "128": "icons/logo.png"
    }
}